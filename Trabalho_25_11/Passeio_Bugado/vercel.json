{
  "version": 2,
  "builds": [
    {
      "src": "front/index.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "back/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/back/server.js" },
    { "src": "/(.*)", "dest": "/front/$1" }
  ]
}
